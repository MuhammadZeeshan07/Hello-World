<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="RegressionSuite">
	<listeners>
		<listener class-name="org.youCater.utils.Listener"></listener>
	</listeners>
	<parameter name="configurations" value="org.youCater.userConfigurationsAndElements.Configurations" />
	<test thread-count="5" name="UserRegressionTestCases">
		<classes>
			<class name="org.youCater.userRegressionTestCases.SignUpTestCases" />
			<class name="org.youCater.userRegressionTestCases.DeleteUserTestCases" />
			<class name="org.youCater.userRegressionTestCases.ForgotPasswordTestCases" />
			<class name="org.youCater.userRegressionTestCases.FiltersTestCases" />
			<class name="org.youCater.userRegressionTestCases.ProfileTestCases" />
			<class name="org.youCater.userRegressionTestCases.PartnerRegistrationTestCases" />
		</classes>
	</test>
</suite>